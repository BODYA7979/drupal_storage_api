function storage_fs_install() {

  // Try to setup a container.

  $container = storage_fs_default_container_create();

  if($container) {
    $class = new StorageClass(variable_get('storage_api_initial_class'));
    
    if($class) {
      $class->addContainer($container);
    }
  }
}


function storage_fs_uninstall() {
}

